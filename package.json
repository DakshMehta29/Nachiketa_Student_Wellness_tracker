{
  "name": "nachiketa-wellness-platform",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:backend": "cd backend && npm run dev",
    "dev:all": "concurrently \"npm:dev\" \"npm:dev:backend\"",
    "build": "vite build",
    "build:backend": "cd backend && npm run build",
    "build:all": "npm run build:backend && npm run build",
    "start": "npm run preview",
    "start:backend": "cd backend && npm start",
    "start:all": "concurrently \"npm:start\" \"npm:start:backend\"",
    "lint": "eslint .",
    "lint:backend": "cd backend && npm run lint",
    "lint:all": "npm run lint && npm run lint:backend",
    "preview": "vite preview",
    "api": "tsx server/index.ts",
    "dev:legacy": "concurrently \"npm:api\" \"npm:dev\""
  },
  "dependencies": {
    "@clerk/clerk-react": "^5.47.0",
    "@supabase/supabase-js": "^2.57.4",
    "@types/body-parser": "^1.19.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "lucide-react": "^0.344.0",
    "node-fetch": "^3.3.2",
    "openai": "^5.20.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.8.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/node": "^22.7.4",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "concurrently": "^9.0.1",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.19.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  }
}
